#include <iostream>

int main() 
{
    const int linhas = 3;
    const int colunas = 3;
    int matriz[linhas][colunas];

    // Preenchendo a matriz com valores fornecidos
    std::cout << "Digite os valores para preencher a matriz 3x3:\n";
    for (int i = 0; i < linhas; i++) 
    {
        for (int j = 0; j < colunas; j++)
        {
            std::cin >> matriz[i][j];
        }
    }

    // Imprimindo a matriz
    std::cout << "Matriz preenchida:\n";
    for (int i = 0; i < linhas; i++) 
    {
        for (int j = 0; j < colunas; j++) 
        {
            std::cout << matriz[i][j] << " ";
        }
        std::cout << std::endl;
    }

    return 0;
}