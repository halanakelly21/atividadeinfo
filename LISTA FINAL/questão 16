#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double numero;

    cout << "Digite um numero: ";
    cin >> numero;

    if (numero > 0) {
        double raizQuadrada = sqrt(numero);
        cout << "Raiz quadrada: " << raizQuadrada << endl;
    } else {
        double quadrado = numero * numero;
        cout << "Quadrado:" << quadrado << endl;
    }

    return 0;
}