#include <iostream>

using namespace std;

int main() {
    int numero;

    cout << "Digite um numero: ";
    cin >> numero;

    if (numero % 10 == 0) {
        cout << "Divisivel por 10." << endl;
    } else if (numero % 5 == 0) {
        cout << "Divisivel por 5." << endl;
    } else if (numero % 2 == 0) {
        cout << "Divisivel por 2." << endl;
    } else {
        cout << "O numero nao e divisivel por nenhum." << endl;
    }

    return 0;
}