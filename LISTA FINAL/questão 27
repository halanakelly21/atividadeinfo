#include <iostream>

using namespace std;

int main() 
{
    int numero;

    cout << "Digite um numero: ";
    cin >> numero;

    int tabuada = 1;

    do {
        int resultado = numero * tabuada;
        cout << numero << " x " << tabuada << " = " << resultado << endl;
        tabuada++;
    } while (tabuada <= 10);

    return 0;
}