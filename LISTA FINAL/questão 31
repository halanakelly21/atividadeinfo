#include <iostream>

using namespace std;

int main()
{
    int numero;

    cout << "Digite um número: ";
    cin >> numero;

    cout << "Tabuada de multiplicação do número " << numero << ":" << endl;

    for (int i = 0; i <= 10; i++) {
        cout << numero << " x " << i << " = " << (numero * i) << endl;
    }

    return 0;
}