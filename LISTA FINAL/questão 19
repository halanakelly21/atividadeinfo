#include <iostream>

using namespace std;

int main() {
    int idade;

    cout << "Digite a idade: ";
    cin >> idade;

    if (idade < 16) {
        cout << "Classe Eleitoral: NÃ£o eleitor" << endl;
    } else if (idade >= 18 && idade <= 65) {
        cout << "Classe Eleitoral: Eleitor obrigatÃ³rio" << endl;
    } else if ((idade >= 16 && idade < 18) || idade > 65) {
        cout << "Classe Eleitoral: Eleitor facultativo" << endl;
    }
    
    return 0;
}